          }
  
          .c-tooltip::before,
          .c-tooltip::after {
              display: block;
              position: absolute;
              left: 50%;
              -webkit-transform: translate(-50%, -10px);
              transform: translate(-50%, -10px);
              opacity: 0;
              transition: .2s;
          }
  
          .c-tooltip::before {
              content: '';
              bottom: calc(100% + 2px);
              border: solid 5px transparent;
              border-top-color: currentColor;
          }
  
          .c-tooltip::after {
              content: attr(aria-label);
              bottom: calc(100% + 12px);
              padding: .61em .93em;
              font-size: .875rem;
              color: #FFF;
              border-radius: 3px;
          }
  
          .c-tooltip:hover::before,
          .c-tooltip:hover::after {
              opacity: 1;
              -webkit-transform: translate(-50%);
              transform: translate(-50%);
          }
  
          footer nav ul li {
              font-size: 1.8rem;
              padding: 10px;
          }
  
